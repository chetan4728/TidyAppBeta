<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#ffff"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="1dp"
        android:background="#ddd"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:id="@+id/notfi"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/mnot"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="15dp"
            android:text="Notifications"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>




    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="1dp"
        android:background="#ddd"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/cartfooter"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/sp_cart"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="My Cart"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/WishListView"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"

            android:layout_marginTop="8dp"
            android:background="@drawable/whishlist"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="My Wishlist"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />




    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/orderList"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/list"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="My Orders"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/myacount"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/account"
            tools:ignore="ContentDescription,RtlHardcoded" />


        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="My Account"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="1dp"
        android:background="#ddd"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/About"
        android:layout_height="match_parent">




        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="About Us"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/TermsConditions"
        android:layout_height="match_parent">




        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="Terms and Conditions"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/Policy"
        android:layout_height="match_parent">




        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="Privacy Policy"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/cancel"
        android:layout_height="match_parent">




        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="Cancel And Returns"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/help_center"
        android:layout_height="match_parent">




        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:text="Help Center"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:id="@+id/rateUs"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"

            android:layout_marginLeft="15dp"
            android:text="Rate Us"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginBottom="15dp"
        android:layout_marginTop="15dp"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:id="@+id/version"
            android:layout_marginLeft="15dp"
            android:textColor="#333"
            android:textSize="15sp"
            android:textStyle="normal"
            tools:ignore="RtlHardcoded" />





    </LinearLayout>



</LinearLayout>