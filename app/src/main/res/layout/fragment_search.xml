<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.tidyhomz.admin.tidyappbeta.View.Fragment.SearchFragment">


   <android.support.v7.widget.SearchView
       android:layout_width="match_parent"
       android:id="@+id/serachLayout"
       android:textColorHint="#878C91"
       android:layout_height="wrap_content"></android.support.v7.widget.SearchView>


   <ListView
       android:id="@+id/list_view_histroy"
       android:layout_marginTop="60dp"
       android:background="#fff"
       android:divider="#F0F0F0"
       android:dividerHeight="1dp"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentStart="true"
       />
   <ListView
       android:id="@+id/list_view"
       android:layout_marginTop="60dp"
       android:background="#fff"
       android:divider="#F0F0F0"
       android:dividerHeight="1dp"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_alignParentStart="true"
       />


</FrameLayout>
